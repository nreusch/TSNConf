	<link src="SW1" dest="SW0" speed="12.50"/>

	<!-- period is in us, size in Byte-->
	<stream name="s0" src="ES0" dest="ES4" size="758" period="5000" type="NORMAL" priority="2" deadline="5000"/>
	<stream name="s1" src="ES0" dest="ES5" size="1318" period="1000" type="NORMAL" priority="3" deadline="1000"/>
	<stream name="s2" src="ES4" dest="ES5" size="1081" period="1000" type="NORMAL" priority="3" deadline="1000"/>
	<stream name="s3" src="ES0" dest="ES3" size="211" period="2000" type="NORMAL" priority="2" deadline="2000"/>
	<stream name="s4" src="ES0" dest="ES2" size="436" period="5000" type="NORMAL" priority="0" deadline="5000"/>
	<stream name="s5" src="ES0" dest="ES4" size="942" period="10000" type="NORMAL" priority="4" deadline="10000"/>
	<stream name="s6" src="ES0" dest="ES5" size="807" period="1000" type="NORMAL" priority="0" deadline="1000"/>

